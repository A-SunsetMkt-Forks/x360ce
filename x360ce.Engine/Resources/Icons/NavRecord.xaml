    <!--  Border  -->
    <Style x:Key="BorderRecord" TargetType="Border">
        <Setter Property="Width" Value="512" />
        <Setter Property="Height" Value="512" />
        <Setter Property="Background" Value="{StaticResource NavColorRecord}" />
        <Setter Property="RenderTransformOrigin" Value="0.5,0.5" />
        <Setter Property="CornerRadius" Value="256" />
    </Style>

    <!--  Arrow Up  -->
    <Style x:Key="ArrowRecord" TargetType="Path">
        <Setter Property="Width" Value="512" />
        <Setter Property="Height" Value="512" />
        <Setter Property="Data" Value="M 307,410 H 205 V 225 H 116 L 256,85 396,225 h -89 z" />
        <Setter Property="Fill" Value="White" />
        <Setter Property="RenderTransformOrigin" Value="0.5,0.5" />
    </Style>

    <Border x:Key="NavRecord" x:Shared="False" Style="{StaticResource BorderRecord}" />

    <Border x:Key="NavUpRecord" x:Shared="False" Style="{StaticResource BorderRecord}">
        <Path Style="{StaticResource ArrowRecord}" />
    </Border>

    <Border x:Key="NavDownRecord" x:Shared="False" Style="{StaticResource BorderRecord}">
        <Path Style="{StaticResource ArrowRecord}">
            <Path.RenderTransform>
                <RotateTransform Angle="180" />
            </Path.RenderTransform>
        </Path>
    </Border>

    <Border x:Key="NavRightRecord" x:Shared="False" Style="{StaticResource BorderRecord}">
        <Path Style="{StaticResource ArrowRecord}">
            <Path.RenderTransform>
                <RotateTransform Angle="90" />
            </Path.RenderTransform>
        </Path>
    </Border>

    <Border x:Key="NavLeftRecord" x:Shared="False" Style="{StaticResource BorderRecord}">
        <Path Style="{StaticResource ArrowRecord}">
            <Path.RenderTransform>
                <RotateTransform Angle="-90" />
            </Path.RenderTransform>
        </Path>
    </Border>

